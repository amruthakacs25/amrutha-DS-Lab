#include <stdio.h>

int n;
int adj[10][10];
int visited[10];
int queue[10];
int front = 0, rear = -1;

void bfs(int start) {
    int v;
    queue[++rear] = start;
    visited[start] = 1;

    while (front <= rear) {
        v = queue[front++];
        printf("%d ", v);

        for (int i = 0; i < n; i++) {
            if (adj[v][i] == 1 && visited[i] == 0) {
                queue[++rear] = i;
                visited[i] = 1;
            }
        }
    }
}

int main() {
    int start;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (int i = 0; i < n; i++) {
        visited[i] = 0;
        for (int j = 0; j < n; j++)
            scanf("%d", &adj[i][j]);
    }

    printf("Enter starting vertex: ");
    scanf("%d", &start);

    printf("BFS traversal: ");
    bfs(start);

    return 0;
}
